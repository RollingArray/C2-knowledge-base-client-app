    ElementTypeEnum["PARA"] = "para";
    ElementTypeEnum["LIST"] = "list";
    ElementTypeEnum["LIST_IMAGE"] = "list-image";
    ElementTypeEnum["PARA_IMAGE"] = "para-image";
})(ElementTypeEnum || (ElementTypeEnum = {}));


/***/ })

}]);
